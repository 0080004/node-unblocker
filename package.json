{
  "name": "node-unblocker",
  "description": "Web proxy for evading internet censorship",
  "author": "Nathan Friedly - http://nfriedly.com",
  "version": "0.14.0",
  "repository": "git://github.com/nfriedly/node-unblocker.git",
  "dependencies": {
    "async": "^1.3.0",
    "connect-redis": "^2.3.0",
    "content-type": "^1.0.1",
    "express": "^4.13.0",
    "express-session": "^1.11.3",
    "gatling": "~1.1.1",
    "iconv-lite": "^0.4.10",
    "mime": "^1.3.4",
    "newrelic": "^1.20.2",
    "redis": "^0.12.1",
    "redis-url": "^1.2.0",
    "through": "~2.3.4",
    "underscore": "^1.8.3"
  },
  "engines": {
    "node": "0.12.x",
    "npm": "2.7.x"
  },
  "devDependencies": {
    "concat-stream": "^1.5.0",
    "dotenv": "^1.2.0",
    "hyperquest": "^1.2.0",
    "js-beautify": "^1.5.10",
    "jshint": "^2.8.0",
    "math-helpers": "~0.1.0",
    "tap": "^1.3.1",
    "taskgroup": "^4.3.0"
  },
  "jshintConfig": {
    "undef": true,
    "unused": true,
    "newcap": true,
    "node": true,
    "browser": false,
    "devel": true
  },
  "todoJshintConfig": {
    "strict": true,
    "camelcase": true
  },
  "scripts": {
    "start": "./node_modules/gatling/gatling.js --threads 2 ./app.js",
    "test": "npm run jshint && npm run test-spec",
    "pre-commit": "npm run beautify && npm test",
    "beautify": "js-beautify --replace ./*.js ./lib/*.js ./test/*.js",
    "jshint": "jshint ./ --exclude node_modules/",
    "test-spec": "tap test/*_spec.js",
    "test-perf": "node test/performance.js"
  },
  "subdomain": "node-unblocker",
  "license": "AGPL"

}
